
{
  "version": 2,
  "builds": [
    {
      "src": "Client/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "Client/dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/Client/dist/$1"
    }
  ],
  "functions": {
    "api/auth.js": {
      "runtime": "nodejs18.x"
    },
    "api/books.js": {
      "runtime": "nodejs18.x"
    },
    "api/cart.js": {
      "runtime": "nodejs18.x"
    },
    "api/orders.js": {
      "runtime": "nodejs18.x"
    }
  },
  "env": {
    "NODE_ENV": "production"
  }
}
